    "data",
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/drigberg/nerdata/issues"
  },
  "homepage": "https://github.com/drigberg/nerdata#readme",
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/joi": "14.0.0",
    "@types/lodash": "4.14.118",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.9",
    "@types/sinon": "5.0.7",
    "chai": "4.2.0",
    "flow-bin": "^0.141.0",
    "flow-coverage-report": "^0.8.0",
